# =============================================================================
# DATABASE CONFIGURATION (PostgreSQL - Primary)
# =============================================================================

# PostgreSQL Database Configuration (Primary Configuration)
POSTGRES_HOST=127.0.0.1
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=bible_db
POSTGRES_WRITE_PASSWORD=postgres

# Database URL for LangChain and SQLAlchemy
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:5432/bible_db
DB_NAME=bible_db

# Database Pool Settings
MIN_CONNECTIONS=1
MAX_CONNECTIONS=10

# AGI System PostgreSQL Configuration
AGI_POSTGRES_HOST=localhost
AGI_POSTGRES_PORT=5433
AGI_POSTGRES_USER=postgres
AGI_POSTGRES_PASSWORD=postgres
AGI_POSTGRES_DB=agi_system_db
AGI_MCP_PORT=5200

